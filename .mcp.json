{
  "mcpServers": {
    "TradingAgentMCP": {
      "command": "uv",
      "args": [
        "run",
        "python",
        "/Users/szou/Python/Playground/TradingAgentMCP/main.py"
      ],
      "env": {
        "LOG_LEVEL": "DEBUG",
        "PYTHONPATH": "/Users/szou/Python/Playground/TradingAgentMCP"
      }
    },
    "serena": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "ide-assistant",
        "--project",
        "/Users/szou/Python/Playground/TradingAgentMCP"
      ],
      "env": {}
    },
    "grep": {
      "type": "http",
      "url": "https://mcp.grep.app"
    },
    "context7": {
      "type": "http",
      "url": "https://mcp.context7.com/mcp"
    },
    "spec-workflow-mcp": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "spec-workflow-mcp@latest"],
      "env": {}
    }
  }
}
